    <crop name="ONION"     returnPeriod="3">
        <good>CANOLA CARROT SUGARBEET WHEAT BARLEY MAIZE OAT</good>
        <bad>ONION POTATO</bad>        
    </crop>
    <crop name="CARROT"    returnPeriod="2">
        <good>MAIZE ONION WHEAT BARLEY OAT</good>
        <bad>SUGARBEET CARROT POTATO</bad>
    </crop>
    
    <crop name="BEETROOT"    returnPeriod="3">
        <good>BARLEY OAT WHEAT SOYBEAN</good>
        <bad>CANOLA MAIZE SUGARBEET BEETROOT</bad>
    </crop>
    
    <crop name="PARSNIP"    returnPeriod="2">
        <good>MAIZE ONION WHEAT BARLEY OAT</good>
        <bad>SUGARBEET CARROT POTATO PARSNIP</bad>
    </crop>
